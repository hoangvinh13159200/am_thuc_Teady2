<script>
  $(".add-cart").click(function(e) {
    const id = $(this).attr("data-id");
    $.ajax({
      url: "/add-to-cart/" + id,
      method: "GET",
      success: data => {
        $(".box-cart").load(" .box-cart > *");
      }
    });
  });
</script>

